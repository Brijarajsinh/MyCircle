<div class="modal-body">
    <div class="row">
        {{#each users}}
        <div class="container card mb-3 col-3">
            <div class="card-body">
                <div class="row">
                    <div class="col-md-2 avatar avatar-m"
                        style="background-image: url(/images/user_images/{{profile}});">
                    </div>
                    <div class="col-md">
                        <div>Full Name:- {{fullName}}</div>
                        <div>E-mail:- {{email}}</div>
                        <div>Total Created POST:- {{postCount}}</div>
                        <div>Total Saved POST:- {{savedCount}}</div>
                    </div>
                </div>
            </div>
            <div class="card-footer bg-transparent">Registered at {{dateConvert createdAt}}</div>
        </div>
        {{/each}}
    </div>
    <nav aria-label="Page navigation example">
        <ul class="pagination justify-content-center">
            {{#each page}}
            <li class="page-item"><a class="page-link user-wise" href="javascript:void(0);"
                    data-page={{this}}>{{this}}</a></li>
            {{/each}}
        </ul>
    </nav>
</div>
<script src="/javascripts/report.js"></script>